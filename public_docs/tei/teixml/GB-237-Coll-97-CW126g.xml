            <div type="series" id="GB-237-Coll-97-CW126g">          
                <!-- REPLACE THIS with the id of the item -->
                <div type="item" id="GB-237-Coll-97-CW126g-1">
                    <!-- REPLACE THIS with the number of the page  -->
                    <pb id="CW126g-222r" n="222r"/>
                    <!-- Pages follow Coll_97_cw_notebook_cw_foliation
                        e.g. cw117-1v refers notebook 117 and folio 1 verso  -->
                    <!-- REPLACE THIS by placing the content of the paragraph in     between the paragraph elements -->
